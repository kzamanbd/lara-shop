{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "insertPragma": false,
    "bracketSameLine": true,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-tailwindcss"]
}
